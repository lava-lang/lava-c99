type Rect {
    int, w, h;

    int area() {
        return w * h;
    }
}

type Widget : Rect {
    void init() {
        //NOOP
    }

    void draw() {
        int x = area();
        c.printf("Area For Widget: %d\n", x);
    }
}

type Label : Widget {
    void override draw() {
        int x = area();
        c.printf("Area For Label: %d\n", x);
    }
}

int main() {
    Widget w = {100, 50};
    w.draw();

    Label l = {25, 10};
    l.draw();
}