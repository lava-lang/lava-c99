type Vec {
    f64 x, y, z;
}

type Sphere {
    Vec c;
    f64 r;
    Vec col;

    void getNormal(Vec* out, Vec* p) {
        out.x = p.x;
        out.y = p.y;
        out.z = p.z;
        out.sub(c);
        out.div(r);
    }
}